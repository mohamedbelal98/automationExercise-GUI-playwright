{
  "name": "automationexercise-gui-playwright",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ui-mode": "npx playwright test --ui",
    "report": "npx playwright show-report",
    "register-testcases": "npx playwright test register.spec.ts --project=chromium",
    "e2eDev": "powershell -Command \"$env:ENV='dev'; npx playwright test --project='chromium' $args\"",
    "e2eProd": "powershell -Command \"$env:ENV='prod'; npx playwright test --project='chromium' $args\"",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.48.2",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "playwright": "^1.49.1"
  }
